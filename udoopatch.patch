+		ret = sitronix_get_id_info(ts, &id[0]);
+		if(ret < 0)
+			return ret;
+		printk("id (hex) = %x %x %x %x\n", id[0], id[1], id[2], id[3]);
+		if((id[0] == 1)&&(id[1] == 2)&&(id[2] == 0xb)&&(id[3] == 1)){
+			return 0;
+		}else{
+			printk("Error: It is not Sitronix IC\n");
+			return -1;
+		}
+	}else{
+		printk("Error: Can not get ID of Sitronix IC\n");
+		return -1;
+	}
