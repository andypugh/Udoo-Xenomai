-		return;
-
-	/* Setting PHY to reset status */
-	hdmi_writeb(HDMI_MC_PHYRSTZ_DEASSERT, HDMI_MC_PHYRSTZ);
-
-	/* Power down PHY */
-	mxc_hdmi_phy_enable_tmds(0);
-	mxc_hdmi_phy_enable_power(0);
-	mxc_hdmi_phy_gen2_txpwron(0);
-	mxc_hdmi_phy_gen2_pddq(1);
